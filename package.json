{
  "name": "download-provisioning-profiles",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "swc src -d lib --delete-dir-on-start -C module.type=commonjs -C jsc.parser.syntax=typescript -C jsc.target=es2019 -s",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --no-cache",
    "dev": "ts-node src/main.ts",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/download-provisioning-profiles"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/io": "^1.1.1",
    "appstore-connect-jwt-generator-core": "^1.0.1",
    "cross-fetch": "^3.1.4",
    "esm": "^3.2.25",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.3.5",
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.106",
    "@swc/helpers": "^0.2.13",
    "@swc/jest": "^0.2.5",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.6",
    "@typescript-eslint/parser": "^5.3.0",
    "@vercel/ncc": "^0.31.1",
    "chokidar": "^3.5.2",
    "eslint": "^8.1.0",
    "eslint-plugin-github": "^4.3.3",
    "eslint-plugin-jest": "^25.2.3",
    "jest": "^27.3.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.4"
  }
}
