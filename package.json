    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.31.1",
    "babel-jest": "^27.2.5",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
