{
  "name": "download-provisioning-profiles",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "./dist/main.mjs",
  "exports": {
    ".": {
      "node": "./dist/main.mjs",
      "default": "./dist/main.mjs",
      "import": "./dist/main.mjs"
    }
  },
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --no-cache",
    "dev": "ts-node src/main.ts",
    "all": "yarn clean && yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/download-provisioning-profiles"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/io": "^1.1.1",
    "appstore-connect-jwt-generator-core": "^1.1.2",
    "cross-fetch": "^3.1.4",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@swc/core": "^1.2.120",
    "@swc/jest": "^0.2.15",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.13",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "^0.33.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.4.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "prettier": "2.5.1",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts-esm",
    "testEnvironment": "node",
    "clearMocks": true,
    "moduleFileExtensions": [
      "mts",
      "mjs",
      "ts",
      "js"
    ],
    "testMatch": [
      "<rootDir>/__tests__/*.test.ts"
    ],
    "transform": {
      "^.+\\.m?(t|j)s$": [
        "ts-jest"
      ]
    },
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/dist/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [
      "node_modules/jest-runtime"
    ],
    "verbose": true
  }
}
