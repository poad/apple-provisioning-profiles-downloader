    "@types/node": "^16.7.2",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/parser": "^4.29.3",
    "@vercel/ncc": "^0.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.2.0",
