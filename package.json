{
  "name": "download-provisioning-profiles",
  "version": "1.0.0",
  "private": true,
  "main": "bin/main.js",
  "scripts": {
    "build": "yarn clean && yarn types && yarn swc",
    "swc": "swc src -d bin",
    "types": "tsc -p tsconfig-types.json",
    "tsc": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "dev": "ts-node src/main.ts",
    "clean": "git clean -Xdf -e '!/.idea/**' -e '!/node_modules' -e '!/node_modules/**' && rimraf bin",
    "clean:dry": "git clean -Xdfn -e '!/.idea/**' -e '!/node_modules' -e '!/node_modules/**'",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "exports": {
    ".": {
      "import": "./bin/main.js",
      "require": "./bin/main.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/download-provisioning-profiles"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "type": "module",
  "author": "Kenji Saito",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/io": "^1.1.1",
    "appstore-connect-jwt-generator-core": "^1.1.4",
    "cross-fetch": "^3.1.4",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.120",
    "@swc/helpers": "^0.3.2",
    "@swc/jest": "^0.2.15",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "^0.33.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.4.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "prettier": "2.5.1",
    "swc-loader": "^0.1.15",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(m|c)?(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "verbose": true
  }
}
