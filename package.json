{
  "name": "download-provisioning-profiles",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "swc --delete-dir-on-start src/*.ts src/**/*.ts  -d lib/",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --no-cache",
    "dev": "ts-node src/main.ts",
    "all": "yarn format && yarn lint && yarn build && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/download-provisioning-profiles"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/io": "^1.1.1",
    "appstore-connect-jwt-generator-core": "^1.0.0",
    "cross-fetch": "^3.1.4",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.3.2",
    "@swc-node/register": "^1.3.5",
    "@swc/cli": "^0.1.49",
    "@swc/core": "^1.2.88",
    "@swc/helpers": "^0.2.13",
    "@swc/jest": "^0.2.3",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.9.2",
    "@typescript-eslint/parser": "^4.31.1",
    "@vercel/ncc": "^0.31.1",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.2.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "prettier": "2.4.1",
    "swc-loader": "^0.1.15",
    "ts-node": "^10.2.1",
    "typescript": "~4.4.3"
  }
}
