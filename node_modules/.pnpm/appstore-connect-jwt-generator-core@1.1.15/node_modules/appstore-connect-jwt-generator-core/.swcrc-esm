{
  "test": ".*/*.(m)?ts$",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false,
      "exportDefaultFrom": false,
      "decoratorsBeforeExport": false
    },
    "target": "es2021"
  },
  "minify": false,
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "sourceMaps": true,
  "inlineSourcesContent": true,
  "exclude": [
    "tests",
    ".*.js$",
    ".*.map$"
  ]
}