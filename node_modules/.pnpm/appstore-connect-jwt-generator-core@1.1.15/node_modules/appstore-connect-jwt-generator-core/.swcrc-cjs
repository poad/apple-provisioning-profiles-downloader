{
  "test": ".*/*.(c)?ts$",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true,
      "exportDefaultFrom": true,
      "decoratorsBeforeExport": true
    },
    "target": "es2021",
    "transform": {
      "optimizer": {
        "jsonify": {
          "minCost": 0
        }
      }
    }
  },
  "minify": false,
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "sourceMaps": true,
  "inlineSourcesContent": true,
  "exclude": [
    "tests",
    ".*.js$",
    ".*.map$"
  ]
}